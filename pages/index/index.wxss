  /* padding: 0 50rpx; */
}

.list .list-item{
  width: 100%;
  height:200rpx;
  display: flex;
  /* margin: 0 10rpx; */
  padding: 0 30rpx;
  align-items: center;

}

.list-item .icon-box{
  display: flex;
  overflow: hidden;
  
}

.list-item .icon{
  width: 150rpx;
  height: 150rpx;
}

.list-item-content{
  margin-left: 25rpx;

}

.list-item-content .title{
  font-size: 33rpx;
  font-weight: 700;
}

.list-item-content .sn{
